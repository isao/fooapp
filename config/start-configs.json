[
    {
        "pattern": "/modules/:name/dimensions.:ext",
        "action": "loadgeneric"
    },
    {
        "pattern": "/pages/",
        "action": "loadpage"
    },
    {
        "pattern": "/conf/dimensions.json$",
        "action": "loadjson",
        "param": ["dimensions"]
    },
    {
        "pattern": "/conf/app.json$",
        "action": "loadjson",
        "param": ["appConfig"]
    },
    {
        "pattern": "/conf/assets.json$",
        "action": "loadjson",
        "param": ["appAssetsConfig"]
    },
    {
        "pattern": "/conf/layouts.json$",
        "action": "loadjson",
        "param": ["layoutYcb"]
    }
]